{
  "name": "echarts-offline-doc-cli",
  "version": "0.0.1",
  "description": "A CLI tool for offline documentation of Apache ECharts.",
  "main": "index.js",
  "scripts": {
    "build": "node ./bin/echarts-offline-doc.js",
    "start": "npx opener dist/index.html",
    "start:server": "npx http-server ./dist -o -c-1 --cors -g"
  },
  "bin": {
    "echarts-offline-doc": "./bin/echarts-offline-doc.js"
  },
  "keywords": [
    "echarts",
    "echarts-doc",
    "echarts-cli",
    "offline"
  ],
  "author": "plainheart <yhen@all-my-life.cn>",
  "license": "MIT",
  "devDependencies": {
    "chalk": "^4.1.2",
    "commander": "^10.0.0",
    "cross-spawn": "^7.0.3",
    "download": "^8.0.0",
    "fs-extra": "^11.1.1",
    "globby": "^11.1.0",
    "spinnies": "^0.5.1"
  }
}
